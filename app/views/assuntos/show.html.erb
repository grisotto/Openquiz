<p id="notice"><%= notice %></p>

<p>
  <h2><%= @assunto.nome_assunto %></h2>
  <strong>Questoes Estudadas:</strong>
</p>

<p>
  <%=image_tag(@assunto.link_image_url(:thumb)) %>
</p>
<h2>Temas</h2>
<% @assunto.temas.where(:disponivel => '1').each do |tema| %>
  <p>
    <li><%= link_to questao_path, tema %></li>
  </p>
 
<% end %>
<%= link_to 'Cadastrar Nova Questao', new_questao_path, target:'_self'%>
<%= link_to 'Voltar', assuntos_path %>



